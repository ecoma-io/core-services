{
  "$schema": "https://json-schema.org/schema",
  "$id": "Library",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "layer": {
      "type": "string",
      "description": "The layer the library will belong to",
      "enum": ["domains", "interactors", "adapters", "none"],
      "default": "none",
      "x-prompt": "Which layer does the library belong to?"
    }
  },
  "required": ["name"]
}
