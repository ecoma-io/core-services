{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "<%= name %>",
  "sourceRoot": "<%= root %>/<%= name %>/src",
  "projectType": "library",
  "tags": ["type:<%= root %>"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{projectRoot}/coverage/"],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.mjs"
      }
    }
  }
}
